      </item>
   </item>
   <item>
      <extension url="http://hl7.org/fhir/StructureDefinition/questionnaire-unit">
         <valueCoding>
            <system value="http://unitsofmeasure.org"/>
            <code value="mg/L"/>
            <display value="milligram per liter"/>
         </valueCoding>
      </extension>
      <extension url="http://hjemmebehandling.rm.dk/question-abbreviation">
         <valueString value="crp"/>
      </extension>
      <linkId value="urn:uuid:fbfe8c5c-e441-47ec-a475-597ec55d9261"/>
      <code>
         <system value="urn:oid:1.2.208.176.2.1"/>
         <code value="NPU19748"/>
         <display value="C-reaktivt protein [CRP];P"/>
      </code>
      <text value="Indtast den målte CRP værdi"/>
      <type value="quantity"/>
      <required value="false"/>
      <item>
         <linkId value="urn:uuid:371c0e4a-5809-49d4-87d9-d7ca81e53cd5"/>
         <text value="CRP er et udtryk for infektion/betændelse. CRP måles på det udleverede apparat. Se den vedlagte instruktion."/>
         <type value="display"/>
      </item>
   </item>
   <item>
      <extension url="http://hjemmebehandling.rm.dk/question-abbreviation">
         <valueString value="antibiotika"/>
      </extension>
      <linkId value="urn:uuid:dc22ed52-d1c1-4bc7-bc21-35022c51b0f9"/>
      <text value="Har du fået alt den ordinerede antibiotika det sidste døgn?"/>
      <type value="boolean"/>
      <required value="false"/>
      <item>
         <linkId value="urn:uuid:d88b9d71-74b0-465d-967f-8580ebb99a98"/>
         <text value="Hvis du får antibiotika på pumpe skal du svare nej hvis der har været problemer med indløb."/>
         <type value="display"/>
      </item>
   </item>
   <item>
      <extension url="http://hjemmebehandling.rm.dk/question-abbreviation">
         <valueString value="helbredstilstand"/>
      </extension>
      <linkId value="urn:uuid:c551a88a-f73e-4cd8-976f-7b53f85526cd"/>
      <text value="Er din helbredstilstand værre idag sammenlignet med igår?"/>
      <type value="boolean"/>
      <required value="false"/>
      <item>
         <linkId value="urn:uuid:c5be75a8-f2e9-4048-8807-c30a485ae601"/>
         <text value="Hvis den er uændret fra i går skal du svare nej."/>
         <type value="display"/>
      </item>
   </item>
   <item>
      <extension url="http://hjemmebehandling.rm.dk/question-abbreviation">
         <valueString value="nye_symptomer"/>
      </extension>
      <linkId value="urn:uuid:132792dc-5dc6-4e3a-8064-da324cd3526f"/>
      <text value="Er der kommet nye symptomer i det sidste døgn?"/>
      <type value="boolean"/>
      <required value="false"/>
