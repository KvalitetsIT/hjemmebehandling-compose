   <title value="Infektionsmedicinsk spørgeskema"/>
   <status value="active"/>
   <item>
      <extension url="http://hl7.org/fhir/StructureDefinition/questionnaire-unit">
         <valueCoding>
            <system value="http://unitsofmeasure.org"/>
            <code value="Cel"/>
            <display value="grader Celsius"/>
         </valueCoding>
      </extension>
