      <required value="false"/>
      <item>
         <linkId value="urn:uuid:27ed8529-e602-4576-a438-087280fd8f61"/>
         <text value="Hvis dit udslæt er kommet i løbet af dette døgn skal du svare nej"/>
         <type value="display"/>
      </item>
   </item>
