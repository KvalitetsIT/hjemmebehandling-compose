         <valueString value="temperature"/>
      </extension>
      <linkId value="urn:uuid:63dc8443-8012-4c1d-ac40-623c4921869c"/>
      <code>
         <system value="urn:oid:1.2.208.176.2.1"/>
         <code value="NPU08676"/>
         <display value="Legeme temp.;Pt"/>
      </code>
      <text value="Indtast din morgen temperatur?"/>
      <type value="quantity"/>
      <required value="false"/>
      <item>
         <linkId value="urn:uuid:bf2f2218-9d70-47c9-92d5-e09c992d3f4b"/>
         <text value="For at få den korrekte værdi skal der være en rektal temperatur dvs. målt i endetarmen"/>
